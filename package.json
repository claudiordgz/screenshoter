{
  "name": "screenshoter",
  "version": "0.0.1",
  "description": "Takes a screenshot of a website, save it to cloudinary",
  "main": "handler.js",
  "scripts": {
    "dev": "webpack --watch",
    "build": "webpack",
    "test": "jest --watch"
  },
  "keywords": [
    "aws",
    "lambda",
    "typescript"
  ],
  "author": "claudiordgz <claudio.rdgz@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^21.1.6",
    "@types/node": "^8.0.53",
    "aws-sdk": "^2.153.0",
    "jest": "^21.2.1",
    "jest-webpack": "^0.3.0",
    "serverless-webpack": "^4.0.0",
    "ts-jest": "^21.2.3",
    "ts-loader": "^3.1.1",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.1",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
